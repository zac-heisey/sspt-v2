<?xml version="1.0" encoding="UTF-8"?>
<urlset
      xmlns="http://www.sitemaps.org/schemas/sitemap/0.9"
      xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
      xsi:schemaLocation="http://www.sitemaps.org/schemas/sitemap/0.9
            http://www.sitemaps.org/schemas/sitemap/0.9/sitemap.xsd">

<!-- Pages -->
  <url>
    <loc>https://www.silverstrandpt.com/</loc>
    <changefreq>monthly</changefreq>
    <priority>1.00</priority>
  </url>
  <url>
    <loc>https://www.silverstrandpt.com/staff</loc>
    <changefreq>monthly</changefreq>
    <priority>0.80</priority>
  </url>
  <url>
    <loc>https://www.silverstrandpt.com/services</loc>
    <changefreq>monthly</changefreq>
    <priority>0.80</priority>
  </url>
  <url>
    <loc>https://www.silverstrandpt.com/insurance</loc>
    <changefreq>monthly</changefreq>
    <priority>0.80</priority>
  </url>
  <url>
    <loc>https://www.silverstrandpt.com/service-areas</loc>
    <changefreq>monthly</changefreq>
    <priority>0.80</priority>
  </url>
  <url>
    <loc>https://www.silverstrandpt.com/privacy-policy/</loc>
    <changefreq>yearly</changefreq>
    <priority>0.50</priority>
  </url>
  <url>
    <loc>https://www.silverstrandpt.com/thank-you/</loc>
    <changefreq>yearly</changefreq>
    <priority>0.50</priority>
  </url>
  <url>
    <loc>https://www.silverstrandpt.com/sitemap/</loc>
    <changefreq>weekly</changefreq>
    <priority>0.80</priority>
  </url>
  <url>
    <loc>https://www.silverstrandpt.com/feed.xml/</loc>
    <changefreq>weekly</changefreq>
    <priority>0.80</priority>
  </url>
  <url>
    <loc>https://www.silverstrandpt.com/blog/</loc>
    <changefreq>weekly</changefreq>
    <priority>0.80</priority>
  </url>

  <!-- Blog Posts -->
  {% for post in site.categories.blog %}
  <url>
    <loc>{{ post.url | prepend: site.baseurl | prepend: site.url }}</loc>
    <lastmod>{{ post.date | date_to_xmlschema }}</lastmod>
    <changefreq>weekly</changefreq>
    <priority>0.65</priority>
  </url>
  {% endfor %}

</urlset>
